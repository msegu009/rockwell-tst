      <div class="test2">

      <label>Table Number:  </label>
        <span id='chingy'>
          <%= select_tag 'table_id', options_for_select(@tables.map{ |t| [t.id, t.min_price] }, {:onchange=> "alert('Hello');"}) %>
        </span>
      </div>
      <br>

      <div class="test2">
        <label>Table Deposit</label>
        <input id="results" type="text" value="" readonly>
      </div>

      <div class="test2">
