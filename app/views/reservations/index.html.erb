        <input id="aresults" type="text" value="" readonly>
        <span id="yaus">
          <%= f.radio_button(:atmosphere, "Yes", value: true, class:"radio-btn", id:"radioyes", checked:( @booking.atmosphere && @booking.atmosphere == true ) ? true : false ) %> Yes <br>
        </span>

        <span id="naus">
          <%= f.radio_button(:atmosphere, "No", value: false, class:"radio-btn", checked: ( @booking.atmosphere && @booking.atmosphere==false ) ? true : false ) %> NO <br>
        </span>
      </div>
      <br>
      <br>

      <div class="test2">
