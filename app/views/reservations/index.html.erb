  <!-- first section -->

  <div class="booking_info">
    <div class="booking_info1">
      <p class="customer_name">Hello <%= @current_user ? @current_user.firstname : "Customer" %> </p> <!-- TODO fix this-->
      <br>
    </div>

    <% #if params[:day] %>
      <%= form_for @booking do |f| %>
        <%= label_tag "Date"%>
           <%= date_field_tag :day %>
           <div class="check_tables_button gray">
           <button id="date-changer" type="button">Check Table Availability</button>
           <input id="clubname-hidden" type="hidden" value="<%= @club.name %>" />
         </div>
        <!-- <p>Button To check Tables</p> -->
        <br>

        <label>Table Number: </label>
        <span id='chingy'>
        <%= select_tag 'table_id', options_for_select(@tables.map{ |t| [t.id, t.min_price] }, {:onchange=> "alert('Hello');"}) %>
        </span>
        <br>

        <label>Table Deposit</label>
        <input id="results" type="text" value="" readonly>

        <p>Group Info:</p>
        <label>Number of Males :</label>
        <span id="mels">
        <input id="nresults" type="text" >
        </span>
        <br>

        <%= f.label :num_of_females, "Number of Females" %>:
        <%= f.text_field :num_of_females %><br />
        <br>

        <%= f.label :atmosphere %> :<br />
        <input id="aresults" type="text" value="" readonly>
        <span id="yaus">
        <%= f.radio_button(:atmosphere, "Yes", value: true, class:"radio-btn", id:"radioyes", checked:( @booking.atmosphere && @booking.atmosphere == true ) ? true : false ) %> Yes <br>
        </span>
        <span id="naus">
        <%= f.radio_button(:atmosphere, "No", value: false, class:"radio-btn", checked: ( @booking.atmosphere && @booking.atmosphere==false ) ? true : false ) %> NO <br>
        </span>
        <br>
        <br>

        <label>Server Request: </label>
        <span id='yonce'>
        <%= select_tag 'server_id', options_for_select(@servers.map{ |s| s.name }) %>
        </span>
        <br>

        <label>Server Deposit</label>
        <input id="sresults" type="text" value="" readonly>
        <br>

        <label>Total</label>
        <span id="finale">
        <input id="total" type="text" value="" readonly>
        </span>
        <a href="#" id="refresh">Reservation Total</a>
        <br>

        <%#= f.submit "Reserve Table" %>
      <% end %>

      <script src="https://checkout.stripe.com/checkout.js" > </script>
      <button id="stripetotal">Purchase Reservation</script>


  </div>


